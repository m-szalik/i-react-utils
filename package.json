{
  "name": "i-react-utils",
  "version": "2.0.0",
  "description": "iReact utils",
  "license": "MIT",
  "author": "m-szalik",
  "main": "lib/index.js",
  "keywords": [
    "react",
    "reactjs",
    "component",
    "javascript",
    "react-component",
    "javascript-utils"
  ],
  "bugs": {
    "url": "https://github.com/m-szalik/i-react-utils/issues"
  },
  "homepage": "https://github.com/m-szalik/i-react-utils/blob/master/README.md",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "i-js-utils": "1.x.x",
    "react": "^15.x.x",
    "react-router": "2.x.x || 3.x.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.x",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "assert": "*",
    "axios": "^0.19.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^9.1.x",
    "babel-plugin-add-module-exports": "*",
    "babel-plugin-react-transform": "*",
    "babel-polyfill": "^6.20.0",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "codecov": "^3.0.0",
    "envify": "*",
    "es6-promise": "*",
    "expose-loader": "*",
    "jest": "^24.8.0",
    "react-addons-test-utils": "^15.x.x",
    "react-dom": "^15.x.x",
    "react-test-renderer": "^15.4.1",
    "rimraf": "^2.7.0",
    "transform-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "scripts": {
    "clean": "rimraf lib dist",
    "test": "jest --watchAll",
    "build": "./node_modules/@babel/cli/bin/babel.js --plugins add-module-exports --out-dir ./lib --no-comments --compact true --minified ./src/",
    "prepublish": "npm run clean && npm run build",
    "start": "node ./example/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m-szalik/i-react-utils"
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": ["./test-env-setup.js"],
    "collectCoverage": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
